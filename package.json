{
  "name": "get-package-json",
  "version": "0.0.0",
  "description": "Get a package.json, from either a npm registry or github",
  "main": "dist/index.js",
  "scripts": {
    "test": "babel-tape-runner test.js && semistandard | snazzy",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build",
    "posttest": "readme package.json > readme.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/get-package-json.git"
  },
  "author": "David BjÃ¶rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kesla/get-package-json/issues"
  },
  "homepage": "https://github.com/kesla/get-package-json#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-tape-runner": "^2.0.1",
    "package-json-to-readme": "^1.5.1",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0",
    "tapava": "^2.2.0"
  },
  "dependencies": {
    "get-package-json-from-github": "^1.1.0",
    "get-package-json-from-registry": "^2.0.0",
    "npm-package-arg": "^4.2.0"
  }
}
